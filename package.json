{
  "name": "radweb-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test radweb",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app-server-watch": "tsc -p tsconfig.server.json --watch",
    "radweb-build": "ng build radweb && npm run schemas:build",
    "schemas:build": "tsc -p projects/radweb/tsconfig.schematics.json",
    "schemas:copy:schemas": "copyfiles -V -u 2  projects/radweb/schematics/*/schema.json dist/radweb/",
    "schemas:copy:files": "copyfiles -a -p -V -u 2 projects/radweb/schematics/*/files/** dist/radweb/",
    "schemas:copy:collection": "copyfiles -V -u 3 projects/radweb/schematics/collection.json dist/radweb/schematics/",
    "postschemas:build": "npm run schemas:copy:schemas && npm run schemas:copy:files && npm run schemas:copy:collection",
    "radweb-server-build": "tsc -p projects/radweb-server/tsconfig.json && copyfiles -u 2 projects/radweb-server/package.json dist/radweb-server",
    "radweb-server-mssql-build": "tsc -p projects/radweb-server-mssql/tsconfig.json && copyfiles -u 2 projects/radweb-server-mssql/package.json dist/radweb-server-mssql",
    "radweb-server-postgres-build": "tsc -p projects/radweb-server-postgres/tsconfig.json && copyfiles -u 2 projects/radweb-server-postgres/package.json dist/radweb-server-postgres",
    "cc": "copyfiles -u 2 projects/radweb-server/package.json dist/radweb-server",
    "server-run": "node distServer/src/app/server/server.js",
    "server:debug": "node --inspect --debug-brk distServer/src/app/server/server.js",
    "publish-radweb": "cd projects/radweb && npm version patch && cd.. && cd.. && npm run radweb-build && cd dist/radweb && npm publish --tag pre-alpha",
    "publish-radweb-server": "cd dist/radweb-server && npm publish --tag pre-alpha",
    "publish-radweb-server-postgres": "cd dist/radweb-server-postgres && npm publish --tag pre-alpha"
    
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "@auth0/angular-jwt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mssql": "^4.0.10",
    "@types/pg": "^7.4.11",
    "bootstrap3": "^3.3.5",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^4.2.3",
    "pg": "^7.6.1",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.3.5",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/websql": "0.0.27",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "cp": "^0.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "ts-node-dev": "^1.0.0-pre.30",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
